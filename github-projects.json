         "regex":"",
         "delay":0
      },
      {
         "id":"forks",
         "type":"SelectorText",
         "selector":".muted-link[href$='/network']",
         "parentSelectors":[
            "project"
         ],
         "multiple":false,
         "regex":"",
         "delay":0
      },
      {
         "id":"language",
         "type":"SelectorText",
